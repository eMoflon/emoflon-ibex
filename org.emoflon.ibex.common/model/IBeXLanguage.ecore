<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="IBeXLanguage" nsURI="platform:/plugin/org.emoflon.ibex.common/IBeXLanguage.ecore"
    nsPrefix="org.emoflon.ibex.common.common.language">
  <eClassifiers xsi:type="ecore:EClass" name="IBeXPattern" eSuperTypes="#//IBexNamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="nodes" upperBound="-1"
        eType="#//IBeXNode"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="edges" upperBound="-1"
        eType="#//IBeXEdge"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="graphs" upperBound="-1"
        eType="#//IBeXGraph"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="IBeXGraph" eSuperTypes="#//IBexNamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="nodes" upperBound="-1"
        eType="#//IBeXNode"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="edges" upperBound="-1"
        eType="#//IBeXEdge"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="IBeXNode" eSuperTypes="#//IBexNamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="incomingEdges" upperBound="-1"
        eType="#//IBeXEdge" eOpposite="#//IBeXEdge/targetNode"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="outgoingEdges" upperBound="-1"
        eType="#//IBeXEdge" eOpposite="#//IBeXEdge/sourceNode"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="ecore:EClass platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EClass"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="IBeXEdge">
    <eStructuralFeatures xsi:type="ecore:EReference" name="sourceNode" eType="#//IBeXNode"
        eOpposite="#//IBeXNode/outgoingEdges"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="targetNode" eType="#//IBeXNode"
        eOpposite="#//IBeXNode/incomingEdges"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="ecore:EClass platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EReference"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="IBexNamedElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
  </eClassifiers>
</ecore:EPackage>
