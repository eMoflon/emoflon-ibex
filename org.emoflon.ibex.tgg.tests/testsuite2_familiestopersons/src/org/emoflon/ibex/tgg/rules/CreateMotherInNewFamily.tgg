#using testsuite2_familiestopersons.*
#using AttrCondDefLibrary.*

#rule CreateMotherInNewFamily #with testsuite2_familiestopersons

#source { 
	reg:FamilyRegister {
		++ -families->family
	}	
	
	++ family:Family {
		name := "Test"
		++-mother->m
	}
	
	++ m:FamilyMember
}

#target {
	preg:PersonRegister {
		++ -persons->p
	}
	
	++ p:Female
}

#correspondence {
	reg2reg:RegisterToRegister{
		#src->reg
		#trg->preg
	}
	
	++ m2p:FamilyMemberToPerson {
		#src->m
		#trg->p
	}
}

#attributeConditions {
	 eq_string(p.name, m.name)
}
