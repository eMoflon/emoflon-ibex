#using testsuite2_familiestopersons.*
#using AttrCondDefLibrary.*

#rule CreateDaughterInExistingFamily #with testsuite2_familiestopersons

#source { 
	f:Family {
		++ -daughters->m
	}
	++ m:FamilyMember
}

#target {
	preg:PersonRegister {
		++-persons->p
	}
	
	++ p:Female
}

#correspondence {
	++ p2m:FamilyMemberToPerson {
		#src->m
		#trg->p
	}
}

#attributeConditions {
	eq_string(m.name, p.name)
}
