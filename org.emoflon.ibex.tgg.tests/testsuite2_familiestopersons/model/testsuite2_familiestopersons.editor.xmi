        <op value="++"/>
        <type href="platform:/resource/SimpleFamilies/model/SimpleFamilies.ecore#//FamilyRegister/families"/>
      </linkVariablePatterns>
    </sourcePatterns>
    <sourcePatterns name="family">
      <op value="++"/>
