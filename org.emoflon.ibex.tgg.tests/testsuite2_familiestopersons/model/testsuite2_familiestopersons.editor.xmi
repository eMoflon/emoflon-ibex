      <linkVariablePatterns target="//@rules.0/@sourcePatterns.2">
        <op value="++"/>
        <type href="platform:/resource/SimpleFamilies/model/SimpleFamilies.ecore#//Family/mother"/>
      </linkVariablePatterns>
    </sourcePatterns>
    <sourcePatterns name="m">
      <op value="++"/>
      <type href="platform:/resource/SimpleFamilies/model/SimpleFamilies.ecore#//FamilyMember"/>
    </sourcePatterns>
    <targetPatterns name="preg">
      <type href="platform:/resource/SimplePersons/model/SimplePersons.ecore#//PersonRegister"/>
      <linkVariablePatterns target="//@rules.0/@targetPatterns.1">
        <op value="++"/>
        <type href="platform:/resource/SimplePersons/model/SimplePersons.ecore#//PersonRegister/persons"/>
      </linkVariablePatterns>
    </targetPatterns>
    <targetPatterns name="p">
