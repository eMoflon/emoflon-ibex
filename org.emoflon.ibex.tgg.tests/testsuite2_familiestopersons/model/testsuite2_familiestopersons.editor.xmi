  <rules name="CreateMotherInNewFamily" schema="//@schema">
    <using importedNamespace="testsuite2_familiestopersons.*"/>
    <using importedNamespace="AttrCondDefLibrary.*"/>
    <sourcePatterns name="reg">
      <type href="platform:/resource/SimpleFamilies/model/SimpleFamilies.ecore#//FamilyRegister"/>
