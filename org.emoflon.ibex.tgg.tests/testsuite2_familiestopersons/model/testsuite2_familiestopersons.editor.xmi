    </targetPatterns>
    <correspondencePatterns name="reg2reg" type="//@schema/@correspondenceTypes.0" source="//@rules.0/@sourcePatterns.0" target="//@rules.0/@targetPatterns.0"/>
    <correspondencePatterns name="m2p" type="//@schema/@correspondenceTypes.1" source="//@rules.0/@sourcePatterns.2" target="//@rules.0/@targetPatterns.1">
      <op value="++"/>
    </correspondencePatterns>
  </rules>
  <rules name="HandleRegisters" schema="//@schema">
    <using importedNamespace="testsuite2_familiestopersons.*"/>
    <using importedNamespace="AttrCondDefLibrary.*"/>
    <sourcePatterns name="fr">
      <op value="++"/>
