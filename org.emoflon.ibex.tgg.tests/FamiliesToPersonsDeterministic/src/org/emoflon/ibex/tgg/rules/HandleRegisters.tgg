#using FamiliesToPersonsDeterministic.*
#using AttrCondDefLibrary.*

#rule HandleRegisters #with FamiliesToPersonsDeterministic

#source { 
	++ fr:FamilyRegister
}

#target {
	++ pr:PersonRegister
}

#correspondence {
	++ roots:RegisterToRegister { 
		#src->fr
		#trg->pr
	}
}

#attributeConditions {
	add(1, 2, 3)
}
