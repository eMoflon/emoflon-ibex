#using testsuite1_familiestopersons.*
#using AttrCondDefLibrary.*

#rule ReplaceFatherWithSon #extends FatherToMale #with testsuite1_familiestopersons

#source { 
	fr:FamilyRegister {
		-families->f2
	}
	
	f:Family {
		-father->fm
		++ -sons->son
		
	}
	f2:Family {
		++ -father->fm
	}
	
	fm:FamilyMember
	
	++ son:FamilyMember {
		name := "son"
	}
}

#target {
	pr:PersonRegister {
		-persons->p
	}
	
	p:Male
}

#correspondence {	
	memberToPerson:FamilyMemberToPerson {
		#src->fm
		#trg->p
	}
}

#attributeConditions {
}
