#using familiestopersons.*
#using AttrCondDefLibrary.*

#rule MotherToFemale #with testsuite1_familiestopersons

#source { 
	fr:FamilyRegister {
		-families->f
	}
	
	f:Family {
		++ -mother->mother
	}
	
	++ mother:FamilyMember {
		name := "mother"
	}
}

#target {
	pr:PersonRegister {
		++ -persons->p
	}
	
	++ p:Female {
		name := "mother"
	}
}

#correspondence {
	roots:testsuite1_familiestopersons.RegisterToRegister {
		#src->fr
		#trg->pr
	}
	
	++ memberToPerson:testsuite1_familiestopersons.FamilyMemberToPerson {
		#src->mother
		#trg->p
	}
}

#attributeConditions {
}
