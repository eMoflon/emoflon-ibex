#using testsuite1_familiestopersons.*
#using AttrCondDefLibrary.*

#rule CreateFourthFamily #with testsuite1_familiestopersons

#source { 
	fr:FamilyRegister {
	 	-families->f1
	 	-families->f2
	 	-families->f3
	 	++ -families->f4
	}
	
	f1:Family {
		name == "Test"
	}
	
	f2:Family {
		name == "Test"
	}
	
	f3:Family {
		name == "Test"
	}
	
	++ f4:Family {
		name := "Test"
	}
}

#target {
	pr:PersonRegister
}

#correspondence {
	fr2pr:RegisterToRegister
	{
		#src->fr
		#trg->pr
	}
}

#attributeConditions {
	
}