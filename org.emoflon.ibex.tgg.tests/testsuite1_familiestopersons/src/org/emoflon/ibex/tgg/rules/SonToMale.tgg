#using familiestopersons.*
#using AttrCondDefLibrary.*

#rule SonToMale #with testsuite1_familiestopersons

#source { 
	fr:FamilyRegister {
		-families->f
	}
	
	f:Family {
		++ -sons->son
	}
	
	++ son:FamilyMember {
		name := "son"
	}
}

#target {
	pr:PersonRegister {
		++ -persons->p
	}
	
	++ p:Male {
		name := "son"
	}
}

#correspondence {
	roots:testsuite1_familiestopersons.RegisterToRegister {
		#src->fr
		#trg->pr
	}
	
	++ memberToPerson:testsuite1_familiestopersons.FamilyMemberToPerson {
		#src->son
		#trg->p
	}
}

#attributeConditions {
}
