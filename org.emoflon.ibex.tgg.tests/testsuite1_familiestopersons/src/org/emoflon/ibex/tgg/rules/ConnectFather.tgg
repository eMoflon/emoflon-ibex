#using testsuite1_familiestopersons.*
#using AttrCondDefLibrary.*

#rule ConnectFather #with testsuite1_familiestopersons

#source { 
	fr:FamilyRegister {
		-families->f
	}
	 
	f:Family {
		++ -father->father
	}
	
	father:FamilyMember {
		name := "father"
	}
}

#target {
	pr:PersonRegister {
		++ -persons->p
	}
	
	++ p:Male {
		name := "father"
	}
}

#correspondence {
	roots:RegisterToRegister {
		#src->fr
		#trg->pr
	}
	
	++ memberToPerson:FamilyMemberToPerson {
		#src->father
		#trg->p
	}
}

#attributeConditions {
}
