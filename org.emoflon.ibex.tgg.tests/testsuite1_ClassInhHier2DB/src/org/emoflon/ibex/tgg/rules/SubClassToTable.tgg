#using testsuite1_ClassInhHier2DB.*
#using AttrCondDefLibrary.*

#rule SubClassToTable #with testsuite1_ClassInhHier2DB

#source {
	++subClazz:Clazz {
		++ - superClass->clazz
		++ - package->classpackage
	}
	classpackage:ClassPackage
	clazz:Clazz {
		- package->classpackage
	}
}

#target {
	table:Table
}

#correspondence {
	clazzToTable:ClazzToTable {
		#src->clazz
		#trg->table
	}
	++ subClazzToTable:ClazzToTable {
		#src->subClazz
		#trg->table
	}
}
